{
  "name": "qustar-pg",
  "version": "0.0.1",
  "description": "pg data source for qustar",
  "license": "MIT",
  "keywords": [
    "qustar",
    "sqlite",
    "pg",
    "sql",
    "typescript"
  ],
  "main": "dist/cjs/src/index.js",
  "module": "dist/mjs/src/index.js",
  "types": "dist/mjs/src/index.d.ts",
  "files": [
    "dist/mjs/src",
    "dist/cjs/src",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/mjs/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "default": "./dist/cjs/src/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tilyupo/qustar.git",
    "directory": "packages/qustar-pg"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && run-s build:*",
    "build:mjs": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "dev": "tsc -w",
    "deploy": "npm run build && node ../../scripts/deploy.mjs",
    "db": "docker run -it --rm -e POSTGRES_USER=qustar -e POSTGRES_PASSWORD=test -p 22783:5432 postgres:12.20-bullseye",
    "test": "vitest run",
    "test:watch": "run-p test:watch:*",
    "test:watch:run": "vitest",
    "test:watch:db": "npm run db"
  },
  "peerDependencies": {
    "pg": "*",
    "qustar": "*"
  },
  "devDependencies": {
    "@types/pg": "^8.11.6",
    "npm-run-all": "^4.1.5",
    "qustar-testsuite": "^0.0.1",
    "rimraf": "^6.0.1",
    "vitest": "^1.6.0"
  }
}