{
    "name": "qustar-datasource-testsuite",
    "version": "0.0.3",
    "description": "Qustar DataSource test suite",
    "main": "build/src/index.js",
    "types": "build/src/index.d.ts",
    "repository": "github:tilyupo/qustar",
    "type": "module",
    "files": [
        "build/src"
    ],
    "exports": {
        ".": "./build/src/index.js"
    },
    "license": "MIT",
    "scripts": {
        "prepublish": "npm run compile",
        "release:major": "npm version $(semver $npm_package_version -i major) && npm publish --tag latest",
        "release:minor": "npm version $(semver $npm_package_version -i minor) && npm publish --tag latest",
        "release:patch": "npm version $(semver $npm_package_version -i patch) && npm publish --tag latest",
        "lint": "gts lint",
        "clean": "gts clean && rimraf coverage",
        "compile": "npm run clean && tsc",
        "fix": "gts fix"
    },
    "devDependencies": {
        "vitest": "^2.0.3"
    },
    "dependencies": {
        "better-sqlite3": "^9.6.0",
        "seedrandom": "^3.0.5",
        "sqlite3": "^5.1.7",
        "ts-pattern": "^5.2.0"
    }
}
