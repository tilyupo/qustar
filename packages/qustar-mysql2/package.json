{
  "name": "qustar-mysql2",
  "version": "0.0.1",
  "description": "mysql2 data source for qustar",
  "license": "MIT",
  "keywords": [
    "qustar",
    "sqlite",
    "mysql2",
    "sql",
    "typescript"
  ],
  "main": "dist/cjs/src/index.js",
  "module": "dist/mjs/src/index.js",
  "files": [
    "dist/src",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/mjs/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "default": "./dist/cjs/src/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tilyupo/qustar.git",
    "directory": "packages/qustar-mysql2"
  },
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && run-s build:*",
    "build:mjs": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "dev": "tsc -w",
    "deploy": "npm run build && node ../../scripts/deploy.js",
    "db": "docker run -it --rm -e MYSQL_ROOT_PASSWORD=test -e MYSQL_USER=qustar -e MYSQL_PASSWORD=test -e MYSQL_DATABASE=qustar -p 22784:3306 mysql:9.0.1",
    "test": "vitest run"
  },
  "peerDependencies": {
    "mysql2": "*",
    "qustar": "*"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "qustar-testsuite": "^0.0.1",
    "rimraf": "^6.0.1",
    "vitest": "^1.6.0"
  }
}