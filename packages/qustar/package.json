{
  "name": "qustar",
  "version": "0.0.1",
  "description": "TypeScript SQL query builder",
  "license": "MIT",
  "keywords": [
    "sql",
    "typescript",
    "db",
    "query",
    "orm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tilyupo/qustar.git",
    "directory": "packages/qustar"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src",
    "src"
  ],
  "exports": {
    ".": "./dist/src/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "deploy": "cp ../../README.md ./ && npm run build && node ../../scripts/deploy.js && rm ./README.md",
    "test": "vitest run"
  },
  "devDependencies": {
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "ts-pattern": "^5.2.0",
    "ts-toolbelt": "^9.6.0"
  }
}
